{
  "name": "js-eval",
  "displayName": "JS-EVAL",
  "publisher": "JSEVAL",
  "icon": "images/JS%20EVAL%20128.png",
  "description": "A Calculator for javaScript, JS",
  "version": "0.2.0",
  "engines": {
    "vscode": "^1.58.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:js-eval.js-eval"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "js-eval.js-eval",
        "title": "JS Eval"
      }
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/audunlk/js-eval.git"
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^8.0.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.58.0",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "@vscode/test-electron": "^2.2.0",
    "eslint": "^8.28.0",
    "glob": "^8.0.3",
    "mocha": "^10.1.0",
    "typescript": "^4.9.3"
  },
  "dependencies": {
    "vm": "^0.1.0"
  }
}
